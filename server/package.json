{
  "scripts": {
    "build": "npx tsc",
    "clean:data": "rm -rf build",
    "docker:cleanup": "docker rm -f car_finder && docker rmi -f carfinder_image",
    "prebuild:docker": "npm run build && cp ../prisma/schema.prisma .",
    "build:docker": "docker build -t carfinder_image .",
    "postbuild:docker": "rm schema.prisma",
    "preinstall": "cp ../prisma/schema.prisma .",
    "postinstall": "npx prisma generate && rm schema.prisma",
    "setup": "docker run --name car-finder -p 5432:5432 -e POSTGRES_PASSWORD=mysecretpassword -d postgres && yarn prisma migrate deploy",
    "start": "npx ts-node carApp.ts"
  },
  "dependencies": {
    "@prisma/client": "^2.24.0",
    "express": "^4.17.1",
    "superagent": "^6.1.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.12",
    "@types/superagent": "^4.1.11",
    "prisma": "^2.24.0",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.2"
  }
}
